{
  "name": "splclient",
  "version": "1.0.0",
  "description": "SPL Client Application",
  "main": "./lib/bundle.js",
  "scripts": {
    "build": "webpack -p --verbose --config webpack/webpack.config.js --progress --profile --colors --display-error-details",
    "build:dev": "webpack-dev-server --display-error-details --progress --config webpack/webpack.config.js --hot --port 3000",
    "start:dev": "cross-env NODE_ENV=development && npm run build:dev",
    "start": "cross-env NODE_ENV=production && npm run build && http-server ./dist -p 3000 -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sumit Hingu",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.9",
    "cross-env": "^1.0.8",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.17.0",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "engines": {
    "node": "4.2.6"
  },
}
